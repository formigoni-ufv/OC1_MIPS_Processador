#! c:/iverilog-x64/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1_1)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00000000008fdb20 .scope module, "testbench" "testbench" 2 81;
 .timescale 0 0;
v00000000008f7650_0 .var "addr", 31 0;
v00000000008f76f0_0 .net "instruction", 31 0, v00000000008fdca0_0;  1 drivers
S_00000000008f74d0 .scope module, "imem" "instructionmemory" 2 85, 3 1 0, S_00000000008fdb20;
 .timescale 0 0;
    .port_info 0 /INPUT 32 "addr"
    .port_info 1 /OUTPUT 32 "instruction"
v00000000021e8840_0 .net "addr", 31 0, v00000000008f7650_0;  1 drivers
v00000000008fdca0_0 .var "instruction", 31 0;
v00000000008fdd40 .array "memory", 2 0, 31 0;
E_00000000021e42b0 .event edge, v00000000021e8840_0;
    .scope S_00000000008f74d0;
T_0 ;
    %pushi/vec4 17399840, 0, 32;
    %ix/load 3, 0, 0;
    %flag_set/imm 4, 0;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v00000000008fdd40, 0, 4;
    %pushi/vec4 17399842, 0, 32;
    %ix/load 3, 1, 0;
    %flag_set/imm 4, 0;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v00000000008fdd40, 0, 4;
    %pushi/vec4 17399844, 0, 32;
    %ix/load 3, 2, 0;
    %flag_set/imm 4, 0;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v00000000008fdd40, 0, 4;
    %end;
    .thread T_0;
    .scope S_00000000008f74d0;
T_1 ;
    %wait E_00000000021e42b0;
    %ix/getv 4, v00000000021e8840_0;
    %load/vec4a v00000000008fdd40, 4;
    %assign/vec4 v00000000008fdca0_0, 0;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_00000000008fdb20;
T_2 ;
    %delay 5, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v00000000008f7650_0, 0, 32;
    %delay 5, 0;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v00000000008f7650_0, 0, 32;
    %delay 5, 0;
    %pushi/vec4 2, 0, 32;
    %store/vec4 v00000000008f7650_0, 0, 32;
    %end;
    .thread T_2;
    .scope S_00000000008fdb20;
T_3 ;
    %wait E_00000000021e42b0;
    %vpi_call 2 97 "$monitor", "%b", v00000000008f76f0_0 {0 0 0};
    %jmp T_3;
    .thread T_3, $push;
# The file index is used to find the file name in the following table.
